{
  "name": "design-sys-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fmt:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "db:generate": "yarn prisma generate",
    "db:migrate": "dotenv -e .env.local -- yarn prisma migrate dev --skip-seed",
    "db:migrate:status": "dotenv -e .env.local -- yarn prisma migrate status",
    "db:migrate:reset": "dotenv -e .env.local -- yarn prisma migrate reset --skip-seed",
    "db:studio": "dotenv -e .env.local -- yarn prisma studio",
    "db:push": "dotenv -e .env.production -- yarn prisma db push",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./public/storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.1.1",
    "axios": "^1.4.0",
    "jsdom": "^22.1.0",
    "next": "13.4.10",
    "next-auth": "^4.22.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-button": "^0.7.2",
    "tailwind-merge": "^1.14.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.2",
    "@storybook/addon-interactions": "^7.2.2",
    "@storybook/addon-links": "^7.2.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.2",
    "@storybook/nextjs": "^7.2.2",
    "@storybook/react": "^7.2.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^3.0.0",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.1.1",
    "storybook": "^7.2.2",
    "tailwindcss": "^3.2.7",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "strip-ansi": "^7.0.1",
    "string-width": "^5.1.2",
    "wrap-ansi": "^8.1.0"
  }
}
