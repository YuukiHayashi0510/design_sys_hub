{
  "name": "design-system-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fmt:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "db:generate": "yarn prisma generate && dotenv -e .env.test -- yarn prisma generate",
    "db:migrate": "yarn prisma migrate dev --skip-seed",
    "db:migrate:test": "dotenv -e .env.test -- yarn prisma migrate dev --skip-seed",
    "db:migrate:status": "yarn prisma migrate status",
    "db:migrate:reset": "yarn prisma migrate reset --skip-seed",
    "db:seed": "yarn prisma db seed",
    "db:studio": "yarn prisma studio",
    "db:studio:test": "dotenv -e .env.test -- yarn prisma studio",
    "db:push:prd": "dotenv -e .env.production -- yarn prisma db push",
    "erd": "prisma generate",
    "test": "dotenv -e .env.test -- yarn jest --runInBand --silent",
    "test:ci": "yarn jest --runInBand --silent",
    "test:watch": "dotenv -e .env.test -- yarn jest --watch --runInBand",
    "storage:init": "docker compose build && docker compose up -d",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./public/storybook"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.1.1",
    "axios": "^1.4.0",
    "jsdom": "^22.1.0",
    "next": "13.4.10",
    "next-auth": "^4.22.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-button": "^0.7.2",
    "react-hook-form": "^7.45.4",
    "tailwind-merge": "^1.14.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@mermaid-js/mermaid-cli": "^10.3.0",
    "@prisma/generator-helper": "^5.1.1",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.4.0",
    "@storybook/nextjs": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.6.2",
    "next-test-api-route-handler": "^3.1.8",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.1.1",
    "prisma-erd-generator": "^1.10.0",
    "storybook": "^7.4.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  }
}
