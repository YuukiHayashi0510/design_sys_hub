{
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.addMissingImports": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    ".next": true
  },
  "cSpell.words": [
    "appbar",
    "autodocs",
    "mypage",
    "nextauth",
    "nextjs",
    "signin"
  ]
}
